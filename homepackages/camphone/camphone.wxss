/* homepackages/camphone/camphone.wxss */
.title {
  width: 200rpx;
  height: calc(100vh - 120rpx);
}

.title .cu-item {
  width: 100%;
  text-align: center;
  background-color: #fff;
  margin: 0;
  border: none;
  height: 50px;
  position: relative;
}

.title .cu-item.cur {
  background-color: #f1f1f1;
}
.title .cu-item.cur::after {
  content: "";
  width: 8rpx;
  height: 30rpx;
  border-radius: 10rpx 0 0 10rpx;
  position: absolute;
  background-color: currentColor;
  top: 0;
  right: 0rpx;
  bottom: 0;
  margin: auto;
}
.phone-container{
  display: flex;
}
.num-container{
  background-color: #f1f1f1;
}